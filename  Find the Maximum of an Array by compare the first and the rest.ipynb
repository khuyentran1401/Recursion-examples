    "def MaxRec(myList):\n",
    "    if len(myList) <= 1:\n",
    "            return myList[0]\n",
    "    else:\n",
    "        m = MaxRec(myList[1:])\n",
    "        return m if m > myList[0] else myList[0]\n",
    "\n",
    "def main():\n",
    "    myList = [int(x) for x in input().split()]\n",
    "    print(\"the largest number is: \", MaxRec(myList))\n",
    "\n",
    "main()"
   ]
